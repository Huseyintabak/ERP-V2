      "id": "warehouse-supabase-tool",
      "name": "Supabase Tool (Warehouse)",
      "type": "n8n-nodes-base.httpRequest",
      "typeVersion": 3,
      "position": [500, 1300],
      "parameters": {
        "method": "GET",
        "url": "={{ $env.SUPABASE_URL }}/rest/v1/{{ $json.toolInput.table }}",
        "queryParametersUi": {
          "parameter": [
            {
