          ]
        },
        "headerParametersUi": {
          "parameter": [
            {
              "name": "apikey",
              "value": "={{ $env.SUPABASE_SERVICE_ROLE_KEY }}"
            },
            {
              "name": "Authorization",
              "value": "={{ `Bearer ${$env.SUPABASE_SERVICE_ROLE_KEY}` }}"
            },
            {
              "name": "Content-Type",
              "value": "application/json"
            }
          ]
